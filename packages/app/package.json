{
  "name": "@codecartographer/app",
  "version": "1.0.0",
  "description": "Electron desktop application for CodeCartographer",
  "main": "dist/main.js",
  "scripts": {
    "build": "pnpm build:main && pnpm build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "dev": "concurrently \"pnpm dev:main\" \"pnpm dev:renderer\"",
    "dev:main": "tsc -p tsconfig.main.json --watch",
    "dev:renderer": "vite",
    "clean": "rm -rf dist",
    "test": "vitest"
  },
  "keywords": ["electron", "desktop", "visualization", "code-analysis"],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.17.1"
}
