{
  "name": "@codecartographer/app",
  "version": "1.0.0",
  "description": "Web interface for CodeCartographer with Express server",
  "main": "dist/server.js",
  "scripts": {
    "build": "pnpm build:server && pnpm build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:client\"",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:client": "vite",
    "start": "node dist/server.js",
    "clean": "rm -rf dist",
    "test": "vitest"
  },
  "keywords": [
    "web",
    "express",
    "visualization",
    "code-analysis"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.17.1",
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "concurrently": "^9.2.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "express": "^5.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zustand": "^5.0.8"
  }
}
